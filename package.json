    "package.json",
    "publish.js",
    "README.md",
    "static",
    "tmpl"
  ],
  "devDependencies": {
    "@jsdoc/salty": "^0.2.5",
    "@playcanvas/eslint-config": "^1.5.0",
    "eslint": "^8.45.0",
    "sass": "^1.64.1"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
